    <body>
        <div class="container">
            <div class="col-sm-12">
                <div class="main-page">
                    {{ render_header() }}
                    <div class="container-fluid">
                        {% for label, flashes in app.session.flashbag.all %}
                            {% for flash in flashes %}
                                <div class="alert alert-{{ label }}">
                                    {{ flash }}
                                </div>
                            {% endfor %}
                        {% endfor %}
                        {% block content %}
                        {% endblock %}
                        {% block afterContent %}
                            {{ render_relases() }}
                            {{ render_new_things() }}
                            {{ render_common_info() }}
                            {{ render_movies_of_the_month() }}
                            {{ render_masters() }}
                        {% endblock %}
                    </div>
                    {{ render_footer() }}
                </div>
            </div>
        </div>
        <script type="text/javascript">
            var orderUrl = '{{ path('order.default.index', [], true) }}';
        </script>
        {% block javascripts %}
            {% javascripts 
                "@FrontendBundle/Resources/public/js/jquery.min.js"
                "@FrontendBundle/Resources/public/js/bootstrap.min.js"
                "@FrontendBundle/Resources/public/js/simpleCart.js"
                "@FrontendBundle/Resources/public/js/mixitup.2.1.2.js"
                "@FrontendBundle/Resources/public/js/application.js"
            %}
            <script type="text/javascript" src="{{ asset_url }}" /></script>
            {% endjavascripts %}
        <!--[if lt IE 9]>
        {% javascripts
            "@FrontendBundle/Resources/public/js/html5shiv.js"
            "@FrontendBundle/Resources/public/js/respond.js"
            "@FrontendBundle/Resources/public/js/sky-tabs-ie8.js"
            "@FrontendBundle/Resources/public/js/jquery.1.9.1.js"
        %}
        <script type="text/javascript" src="{{ asset_url }}" /></script>
