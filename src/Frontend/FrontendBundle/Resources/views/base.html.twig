<!DOCTYPE html>
<html lang="pl">
    <head>
        <meta charset="utf-8">
        {#        {{ sonata_seo_title() }}
        
                {{ sonata_seo_metadatas() }}#}
        {% block meta %}
            <meta name="description" content="{% block seo_describtion %}{% endblock %}">
            <meta name="keywords" content="{% block seo_keywords %}{% endblock %}">
        {% endblock %}
        <title>{% block title %}{% endblock %} Dobra Psychoterapia.net</title>
        <!-- Bootstrap -->
        {% block styles %}
            {% stylesheets 
            "@FrontendBundle/Resources/public/css/bootstrap.min.css"
            "@FrontendBundle/Resources/public/css/font-awesome.css"
            "@FrontendBundle/Resources/public/css/sky-mega-menu.css"
            "@FrontendBundle/Resources/public/css/sky-mega-menu-blue.css"
            "@FrontendBundle/Resources/public/css/sky-tabs.css"
            "@FrontendBundle/Resources/public/css/sky-tabs-blue.css"
            "@FrontendBundle/Resources/public/css/sky-forms.css"
            "@FrontendBundle/Resources/public/css/style.css"
            %}
            <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
            <!--[if lt IE 9]>
            {% stylesheets 
                "@FrontendBundle/Resources/public/css/sky-tabs-ie8.css"
            %}
            <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
            <![endif]-->
        {% endblock %}
